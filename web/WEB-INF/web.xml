<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>dbsetup</servlet-name>
        <servlet-class>Db.dbsetup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Db.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Db.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>Db.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upload_excels</servlet-name>
        <servlet-class>SupplyChain.upload_excels</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_county</servlet-name>
        <servlet-class>Loaders.load_county</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_subcounty</servlet-name>
        <servlet-class>Loaders.load_subcounty</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_facilities</servlet-name>
        <servlet-class>Loaders.load_facilities</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>raw_report</servlet-name>
        <servlet-class>Reports.raw_report</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_elements</servlet-name>
        <servlet-class>Loaders.load_elements</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_profile</servlet-name>
        <servlet-class>Loaders.load_profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_user</servlet-name>
        <servlet-class>Loaders.update_user</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_dashboard</servlet-name>
        <servlet-class>Loaders.load_dashboard</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dbsetup</servlet-name>
        <url-pattern>/dbsetup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload_excels</servlet-name>
        <url-pattern>/upload_excels</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_county</servlet-name>
        <url-pattern>/load_county</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_subcounty</servlet-name>
        <url-pattern>/load_subcounty</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_facilities</servlet-name>
        <url-pattern>/load_facilities</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>raw_report</servlet-name>
        <url-pattern>/raw_report</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_elements</servlet-name>
        <url-pattern>/load_elements</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_profile</servlet-name>
        <url-pattern>/load_profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_user</servlet-name>
        <url-pattern>/update_user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_dashboard</servlet-name>
        <url-pattern>/load_dashboard</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
